{
	"name": "morimorig3.com",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"check": "run-s *:check cspell test",
		"fix": "run-s *:fix",
		"svelte-check:check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"prettier:check": "prettier --plugin-search-dir . --check 'src/**/*.{js,jsx,ts,tsx,svelte}'",
		"eslint:check": "eslint 'src/**/*.{js,jsx,ts,tsx,svelte}'",
		"eslint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,svelte}'",
		"prettier:fix": "prettier --plugin-search-dir . --write 'src/**/*.{js,jsx,ts,tsx,svelte}'",
		"cspell": "cspell -c .cspell.json 'src/**/*.{js,jsx,ts,tsx,svelte}'",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@octokit/types": "^12.0.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/adapter-vercel": "^3.0.3",
		"@sveltejs/kit": "^1.20.4",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.14",
		"cspell": "^6.31.2",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.28.0",
		"eslint-plugin-svelte": "^2.30.0",
		"husky": "^8.0.0",
		"npm-run-all": "^4.1.5",
		"octokit": "^3.1.0",
		"postcss": "^8.4.27",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"rehype-class-names": "^1.0.14",
		"rehype-stringify": "^10.0.0",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.0.0",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"svelte-focus-trap": "^1.2.0",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"unified": "^11.0.3",
		"vite": "^4.4.2",
		"vitest": "^0.32.2"
	},
	"type": "module",
	"dependencies": {
		"axios": "^1.5.0",
		"svelte-gestures": "^1.5.2"
	}
}
